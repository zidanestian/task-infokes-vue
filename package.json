{
  "name": "task-infokes-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:unit": "vitest --environment happy-dom",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "start-server-and-test 'npm run dev' http://localhost:5174 'cypress run'",
    "test:e2e:dev": "start-server-and-test 'npm run dev' http://localhost:5174 'cypress open'"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "bootstrap": "^5.3.6",
    "bootstrap-icons": "^1.13.1",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/vue": "^8.0.2",
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^1.3.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^13.6.6",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "happy-dom": "^13.6.2",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "start-server-and-test": "^2.0.12",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^1.3.1",
    "vue-tsc": "^2.2.8"
  }
}
